{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "hangfen"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "styles.css",
        "../src/assets/css/animate.css",
        "../node_modules/magnificent/src/css/mag.css",
        "../node_modules/magnificent/src/theme/default.css"
      ],
      "scripts": [
        "../node_modules/jquery/dist/jquery.js",
        "../src/assets/js/confirmFunc.js",
        "../node_modules/jquery-mousewheel/jquery.mousewheel.js",
        "../node_modules/jquery-bridget/jquery-bridget.js",
        "../node_modules/jquery.threedubmedia/event.drag/jquery.event.drag.js",
        "../node_modules/magnificent/src/js/mag-analytics.js",
        "../node_modules/magnificent/src/js/mag.js",
        "../node_modules/magnificent/src/js/mag-jquery.js",
        "../node_modules/magnificent/src/js/mag-control.js"

      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"

      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json"
    },
    {
      "project": "src/tsconfig.spec.json"
    },
    {
      "project": "e2e/tsconfig.e2e.json"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "component": {}
  }
}
